		<![CDATA[SELECT
     produto.`idProduto` AS produto_idProduto,
     produto.`nomeProd` AS produto_nomeProd,
     produto.`idCategoria` AS produto_idCategoria,
     produto.`precoCompra` AS produto_precoCompra,
     estoque.`qtde` AS estoque_qtde
